{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "outDir": "dist",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "baseUrl": "./",
    "paths": {
      "src/*": ["./src/*"],
      "@log-to-route/types": ["./src/types/types"],
      "@log-to-route/utils/*": ["./src/utils/*"]
    }
  }
}
